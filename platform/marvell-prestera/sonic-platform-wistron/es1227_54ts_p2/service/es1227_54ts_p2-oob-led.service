[Unit]
Description=Platform es1227_54ts_p2 OOB LED Service
After=pmon.service es1227_54ts_p2-init.service
DefaultDependencies=no

[Service]
ExecStartPre=/usr/bin/sleep 20
ExecStart=/usr/local/bin/es1227_54ts_p2-oob-led.sh
KillSignal=SIGKILL
SuccessExitStatus=SIGKILL
Restart=on-failure
RestartSec=5s
#StandardOutput=tty

[Install]
WantedBy=multi-user.target
